@model WebShop.Models.Files.ExistingFilePathForSpaceshipViewModel

@{
    var photoPaths = "~/multipleFileUpload/" + (Model.FilePath ?? "noimage.jpg"); 
}

<div class="form-group eow col-sm-4 offset-2">
     <img classs="card-img-top" src="@photoPaths" asp-append-version="true" />
    <input asp-action="removeImage" asp-controller="spaceship" asp-route-filePath="@Model.FilePath" type="submit" class="btn btn-danger" value="Remove Image" />
</div>